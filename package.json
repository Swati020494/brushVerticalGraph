  },
  "homepage": "https://github.com/Swati020494/brushVerticalGraph#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.5.0",
    "eslint-watch": "^2.1.14",
    "npm-run-all": "^4.0.2",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "d3": "^4.13.0",
    "d3-tip": "^0.9.1"
